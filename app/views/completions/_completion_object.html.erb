


<!--takes in object a base, and key -->

  <% if object.is_a?(Hash) %>  
  
  <%= render "completions/hash", object: object, base: base, key: key%>
  <%elsif  object.is_a?(Array) %>
    <!--field[completion][rewards_list] = base key = [] -->

  <%= render "completions/list", object: object, base: base, key: key%>
  <%elsif  object.is_a?(String) %>
  <%= render "completions/string", object: object, base: base, key: key %>
  <%elsif  object.is_a?(Integer) %>
  <%= render "completions/integer", object: object, base: base, key: key %>
  <%end %>

  <br>