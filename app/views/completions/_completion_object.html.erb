

<!--takes in value: value and later type -->
<% if value.is_a?(Hash) %>
<%= render "completions/object", object: value, parent: parent%>
<%elsif  value.is_a?(Array) %>
<%= render "completions/list", object: value, parent: parent%>
<%elsif  value.is_a?(String) %>
<%= render "completions/string", object: value, parent: parent%>
<%end %>

