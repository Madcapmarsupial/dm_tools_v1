<%= form_with model: field do |form| %>
   <%= form.hidden_field :quest_id, value: field.quest.id %>
   <%= form.hidden_field :type, value: field.type %>  <!-- model_type[title] -->
    <%= render "completions/full_completion", object: field, base: field.get_type, key_list: field.user_key_list%>

  <%= form.submit %>
<% end %>